            function change(id){
                id= "#"+id;
                const val = document.querySelector(id).innerHTML;
                document.querySelector("#ddName").innerHTML = val;
                document.querySelector("#heading1").innerHTML = "<h5 style= \"margin-left: -12px;\">Registeration Type</h5>";
                if(val === "Self"){
                    $("#ticketCount").attr({
