            function genData(){
                var fn = document.getElementById("first_name").value
                var ln = document.getElementById("last_name").value
                var name = fn + " " + ln
                var phone = document.getElementById("phone").value
                var email = document.getElementById("email").value
                const val = document.getElementById("ddName").innerText
                var tickets = document.getElementById("ticketCount").value
                document.querySelector("#idName").innerHTML = name
                document.querySelector("#idPhone").innerHTML = phone
                document.querySelector("#idMail").innerHTML = email
                document.querySelector("#idCat").innerHTML = val
                document.querySelector("#idTcount").innerHTML = tickets
            }
            $(document).ready(function(){       
                var scroll_start = 0;
                var startchange = $('#changecolor');
                var offset = startchange.offset();
                if (startchange.length){
                $(document).scroll(function() { 
                    scroll_start = $(document).scrollTop();
                    if(scroll_start > offset.top) {
                        $(".navbar").css('background-color', '#222');
                        $(".navbar").addClass("navbar-dark");
                        $(".navbar-brand").removeClass("d-none d-sm-block");
                    } else {
